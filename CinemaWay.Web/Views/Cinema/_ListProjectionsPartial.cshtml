@model IEnumerable<ListProjectionWithMovieModel>

@foreach (var proj in Model)
{
    <div class="col-md-4 col-sm-6 text-center">
        <div class="panel panel-info">
            <div class="panel-heading">
                <div class="panel-title">
                    THEME: @proj.Theme
                </div>
                <h3 class="text-info">
                    <a asp-action="Details" asp-route-id="@proj.Id">@proj.Movie</a>
                </h3>
            </div>
            <div class="panel-body">
                <img src="@proj.MovieImage" alt="@proj.Movie" class="img-responsive" style="height: 300px;" />
            </div>
        </div>
    </div>
}